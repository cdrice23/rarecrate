{
  "name": "rarecrate",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "dev:build": "dotenv -e .env.development -- next build",
    "start": "next start",
    "format": "prettier --config .prettierrc './**/*.ts' './**/*.tsx' '.storybook/*' --write",
    "lint": "yarn lint:eslint && yarn lint:types && yarn lint:format",
    "lint:eslint": "eslint ./ --ext .js,.jsx,.ts,.tsx --cache --cache-strategy content --cache-location .eslintcache",
    "lint:types": "tsc --noEmit",
    "lint:format": "prettier --config .prettierrc './**/*.ts' './**/*.tsx' '.storybook/*' --check",
    "test:ci": "jest --ci --coverage --passWithNoTests",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@types/node": "20.3.2",
    "@types/react": "18.2.14",
    "@types/react-dom": "18.2.6",
    "dotenv": "^16.3.1",
    "eslint": "8.43.0",
    "eslint-config-next": "13.4.7",
    "next": "13.4.7",
    "prisma": "^4.16.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "sass": "^1.63.6",
    "typescript": "5.1.5"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^7.0.24",
    "@storybook/addon-interactions": "^7.0.24",
    "@storybook/addon-links": "^7.0.24",
    "@storybook/blocks": "^7.0.24",
    "@storybook/nextjs": "^7.0.24",
    "@storybook/react": "^7.0.24",
    "@storybook/testing-library": "^0.0.14-next.2",
    "eslint-plugin-storybook": "^0.6.12",
    "jest": "^29.5.0",
    "prettier": "2.8.8",
    "storybook": "^7.0.24"
  }
}
